    ability.sync(function (err) {
        if (err) throw err;
        for (i = 0; i < abilitiesArray.length; i++) {
            ability.create({
                name: abilitiesArray[i][0],
                description: abilitiesArray[i][1]
            }, function (err) {
                if (err) throw err;
            });

        }
    });